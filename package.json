{
    "private": true,
    "type": "commonjs",
    "browserslist": "> 0.2%, last 2 versions, Firefox ESR, not dead",
    "scripts": {
        "dev": "run-p routes lint && vite",
        "build:dev": "run-p routes lint && vite build --mode development",
        "build:prod": "run-p routes lint && vite build",
        "build:typescript": "run-p routes",
        "build:css": "postcss --use cssnano postcss-import -o public/app.css resources/css/app.css",
        "lint": "eslint \"resources/js/**/*.{js,ts,vue}\" . --fix",
        "test:lint": "eslint \"resources/js/**/*.{js,ts,vue}\" .",
        "routes": "php artisan trail:generate",
        "type-check": "vue-tsc --noEmit"
    },
    "dependencies": {
        "@heroicons/vue": "^2.0.18",
        "@inertiajs/vue3": "^1.0.8",
        "axios": "^1.4.0",
        "mini-svg-data-uri": "^1.4.4",
        "momentum-layout": "^0.1.0",
        "momentum-trail": "^0.3.0"
    },
    "devDependencies": {
        "@babel/parser": "^7.22.5",
        "@tailwindcss/aspect-ratio": "^0.4.0",
        "@tailwindcss/forms": "^0.5.3",
        "@tailwindcss/typography": "^0.5.9",
        "@types/ziggy-js": "^1.3.2",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "@vitejs/plugin-vue": "^4.2.3",
        "@vue/typescript": "^1.8.2",
        "autoprefixer": "^10.4.14",
        "cssnano": "^6.0.1",
        "eslint": "^8.44.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-vue": "^9.16.1",
        "laravel-vite-plugin": "^0.7.8",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.24",
        "postcss-cli": "^10.1.0",
        "postcss-import": "^15.1.0",
        "prettier": "^3.0.0",
        "prettier-plugin-organize-imports": "^3.2.2",
        "prettier-plugin-tailwindcss": "^0.4.0",
        "pusher-js": "^8.3.0",
        "tailwindcss": "^3.3.2",
        "typescript": "^5.1.3",
        "unplugin-auto-import": "^0.16.4",
        "unplugin-vue-components": "^0.25.1",
        "vite": "^4.3.9",
        "vite-plugin-manifest-sri": "^0.1.0",
        "vite-plugin-watch": "^0.2.0",
